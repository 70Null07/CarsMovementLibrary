    <PackageReference Include="System.Drawing.Common" Version="8.0.2" />
    <PackageReference Include="YoloV8" Version="3.1.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MediaFileProcessor">
      <HintPath>..\..\MediaFileProcessor\MediaFileProcessor\MediaFileProcessor\bin\Debug\netstandard2.1\MediaFileProcessor.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="best.onnx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
